{
  "name": "business_card",
  "version": "1.0.0",
  "description": "A business card application that reads OCR business contacts and outputs the desired extracted fields",
  "main": "app/server/server.js",
  "engines": {
    "npm": ">=5",
    "node": ">=10.15.0"
  },
  "scripts": {
    "start": "node app/server/server.js",
    "lint": "eslint app/**/*.js",
    "test": "cross-env BABEL_ENV=cjs nyc --reporter=text --reporter=html mocha \"app/**/*.test.js\" --require @babel/register --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacharybergmann/business_card.git"
  },
  "author": "Zachary Bergmann",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/zacharybergmann/business_card/issues"
  },
  "homepage": "https://github.com/zacharybergmann/business_card#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jsdom": "^15.2.0",
    "jsdom-simulant": "^1.1.2",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "app/**/*.js"
    ],
    "exclude": [
      "app/**/*Config.js",
      "app/**/*.test.js"
    ],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "check-coverage": true
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "pug": "^2.0.4"
  }
}
